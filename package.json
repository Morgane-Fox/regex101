{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "regex101",
  "title": "Regex101",
  "description": "Interacts with Regex101 website",
  "icon": "command-icon.png",
  "author": "MorganeFox",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "clipboard-to-test",
      "title": "clipboard-to-test",
      "description": "Opens the last copied item into Regex101 (as a test string)",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.34.1",
    "axios": "0.27.2",
    "run-applescript": "6.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
